     "data": {
      "image/png": "",
      "text/latex": [
       "$\\displaystyle \\left\\{\\left( 1 - 7 v_{3}, \\  2 v_{3} - 1, \\  v_{3}\\right)\\right\\}$"
      ],
      "text/plain": [
       "{(1 - 7⋅v₃, 2⋅v₃ - 1, v₃)}"
      ]
     },
     "execution_count": 9,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "linsolve(A, (v1, v2, v3))"
